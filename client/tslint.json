{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "indent": [true, "tabs", 2],
        "no-console": false,
        "object-literal-sort-keys": false,
        "max-line-length": false,
        "variable-name": [
            true, 
            "ban-keywords", 
            "check-format", 
            "allow-snake-case",
            "allow-pascal-case"
        ],
        "forin": false
    },
    "rulesDirectory": []
}